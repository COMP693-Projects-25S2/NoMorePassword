{
    "logout_optimization": {
        "enabled": true,
        "timeout_settings": {
            "first_logout_timeout": 15,
            "subsequent_logout_timeout": 8,
            "feedback_check_interval": 0.2,
            "max_wait_time": 30
        },
        "connection_pool": {
            "pre_initialize": true,
            "cache_connection_validity": true,
            "cache_duration_seconds": 5,
            "parallel_processing": true
        },
        "c_client_optimization": {
            "incremental_cleanup": true,
            "repeated_logout_threshold_seconds": 30,
            "async_processing": true,
            "immediate_feedback": true
        },
        "performance_targets": {
            "first_logout_max_seconds": 15,
            "subsequent_logout_max_seconds": 8,
            "feedback_response_max_seconds": 3
        },
        "logging": {
            "enable_performance_logging": true,
            "log_timeout_events": true,
            "log_optimization_metrics": true
        }
    }
}