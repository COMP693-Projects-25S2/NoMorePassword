{
  "name": "nmp-system",
  "version": "1.0.0",
  "description": "NMP (No More Password) - B-Client and C-Client System",
  "main": "src/main/c-client/main.js",
  "scripts": {
    "start": "electron .",
    "start:c-client": "electron src/main/c-client/main.js --c-client",
    "start:b-client": "electron src/main/discard-b/bClientApp.js --b-client",
    "dev": "electron . --dev",
    "dev:c-client": "electron src/main/c-client/main.js --dev --c-client",
    "dev:b-client": "electron src/main/discard-b/bClientApp.js --dev --b-client",
    "build": "electron-builder",
    "build:exe": "node build-exe-c-client.js",
    "build:electron-exe": "node build-electron-exe.js",
    "build:simple-exe": "node build-simple-exe.js",
    "build:vs": "node build-with-vs-tools.js",
    "build:desktop": "node build-desktop-exe.js",
    "build:desktop-simple": "node build-desktop-simple.js",
    "dist": "npm run build",
    "dist:exe": "npm run build:exe",
    "dist:electron-exe": "npm run build:electron-exe",
    "dist:simple-exe": "npm run build:simple-exe",
    "dist:vs": "npm run build:vs",
    "dist:desktop": "npm run build:desktop",
    "dist:desktop-simple": "npm run build:desktop-simple",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "postinstall": "electron-builder install-app-deps",
    "lint": "echo \"No linter configured\"",
    "audit:fix": "npm audit fix",
    "fix-modules": "npx electron-rebuild"
  },
  "devDependencies": {
    "electron": "^37.3.1",
    "electron-builder": "latest",
    "electron-rebuild": "^3.2.9",
    "electron-reload": "^2.0.0-alpha.1",
    "rimraf": "^5.0.0"
  },
  "keywords": [
    "electron",
    "browser",
    "history",
    "tracking"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mysql2": "^3.14.5",
    "node-fetch": "^3.3.2",
    "sqlite3": "^5.1.7",
    "uuid": "^10.0.0",
    "ws": "^8.14.2"
  }
}